import numpy as np
import matplotlib.pyplot as plt

x = np.linspace(1, 10, 400)
y = (1/x) * np.cos(x**2 + 1/x)

plt.figure(figsize=(8, 5))
plt.plot(x, y, linewidth=2, color='blue', label="Y(x) = (1/x)*cos(x^2 + 1/x)")
plt.xlabel("x")
plt.ylabel("Y(x)")
plt.title("Графік функції Y(x)")
plt.legend()
plt.grid(True)

plt.show()
